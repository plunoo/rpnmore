{
  "name": "rpnmore-website",
  "type": "static",
  "buildPath": "/",
  "domain": "rpnmore.com",
  "customHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "redirects": [
    {
      "source": "/privacy",
      "destination": "/privacy-policy.html",
      "permanent": true
    }
  ]
}